<h1>Register user</h1>
<div class="form-group">
    <label for="tb-email">Email: </label>
    <input id="tb-email" class="form-control">
</div>
<div class="form-group">
    <label for="tb-password">Password: </label>
    <input id="tb-password" type="password" class="form-control">
</div>
<button class="btn btn-default" id="btn-register">Register</button>
<button class="btn btn-default" id="btn-login">Login</button>
<button class="btn btn-danger hidden" id="btn-logout">Logout</button>
